{
  "name": "hello-color",
  "version": "1.0.1",
  "description": "Functional color palette generator",
  "main": "dist/index.js",
  "browser": "dist/umd.js",
  "scripts": {
    "prepublish": "babel src --out-dir dist && webpack -p --progress --colors",
    "build": "webpack -p --progress --colors --config demo/webpack.config.js",
    "gh-pages": "gh-pages -d demo",
    "start": "webpack-dev-server --progress --colors --config demo/webpack.config.js",
    "test": "ava"
  },
  "author": "Brent Jackson",
  "license": "MIT",
  "dependencies": {
    "chroma-js": "^1.2.1"
  },
  "devDependencies": {
    "@jxnblk/bikeshed": "^1.0.0-beta.1",
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "color": "^0.11.3",
    "gh-pages": "^0.11.0",
    "h0": "^1.0.0-beta.4",
    "morphdom": "^2.0.2",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
